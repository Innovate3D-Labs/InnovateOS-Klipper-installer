    "@playwright/test": "^1.40.1",
    "@rushstack/eslint-patch": "^1.6.1",
    "@tsconfig/node18": "^18.2.2",
    "@types/jsdom": "^21.1.6",
    "@types/node": "^20.10.4",
    "@types/websocket": "^1.0.10",
